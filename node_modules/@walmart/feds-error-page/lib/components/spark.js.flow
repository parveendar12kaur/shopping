import React from "react";
import PropTypes from "prop-types";
import classNames from "classnames";

const Spark = ({ size, className, ...otherProps }) => {
  return (
    <span
      className={classNames("elc-icon elc-icon-spark", "error-page__spark", className)}
      style={{ fontSize: size }}
      {...otherProps}
    />
  );
};

Spark.displayName = "Spark";

Spark.propTypes = {
  size: PropTypes.number
};

Spark.defaultProps = {
  size: null
};

export default Spark;
